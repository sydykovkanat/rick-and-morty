{
	"trailingComma": "all",
	"tabWidth": 2,
	"useTabs": true,
	"semi": true,
	"singleQuote": true,
	"jsxSingleQuote": true,
	"arrowParens": "always",
	"importOrderSeparation": true,
	"importOrderSortSpecifiers": true,
	"importOrderCaseInsensitive": true,
	"importOrder": [
		"<THIRD_PARTY_MODULES>",
		"^@/api/(.*)$",
		"^@/app/(.*)$",
		"^@/components/(.*)$",
		"^@/config/(.*)$",
		"^@/constants/(.*)$",
		"^@/hooks/(.*)$",
		"^@/services/(.*)$",
		"^@/shared/(.*)$",
		"^@/store/(.*)$",
		"^@/utils/(.*)$",
		"^../(.*)$",
		"^./(.*)$",
		"(.scss)$"
	],
	"plugins": ["@trivago/prettier-plugin-sort-imports"]
}
